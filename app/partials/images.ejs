<div class="panel active">
  <div class="panel-handle">
    <span>Images</span>
  </div>
  <div class="panel-content">

    <div class="panel-header">
      <div class="buttonbar">
        <a class="button" data-toggle="modal" data-target="#newImageModal" data-bind="click: function() { editingImage(new Image()) }">Create</a>
        <a class="button" data-bind="click: function() { console.log(JSON.parse(ko.toJSON(activeImage().sizes()[1], null, 2))) }">Debug</a>
        <a class="button modal-trigger" data-target="#newImageModal">Foo</a>
      </div>
    </div>

    <ul class="imagesList" data-bind="foreach: images, click: function() { $root.clearActiveImage() }">
      <li data-bind="click: function() { $root.setActiveImage(id) }, clickBubble: false, css: { active: active() }">
        <div class="imagesList-name">
          <span data-bind="visible: !isEditing(name)">
            <span data-bind="text: name, click: function() { editing(name) }"></span>
          </span>
          <span data-bind="visibleAndSelect: isEditing(name)">
            <input class="form-control" type="text" data-bind="value: name, event: { blur: function() { clearEdit(name) } }">
          </span>
        </div>
        <a class="imagesList-remove" data-bind="click: function() { $root.removeImage(id) }">&times;</a>
      </li>
    </ul>

  </div>
</div>